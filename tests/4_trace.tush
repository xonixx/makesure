$ cd "$MYDIR"; ./$MAKESURE -f tests/4_trace.sh -x         2>&1 | awk -f tests/unify_tracing_output.awk
| + : default@reached_if
| + MYDIR=/some/path/tests
| + export MYDIR
| + cd /some/path/tests
| + A=aaa
| + export A
| + [[ 1 -eq 2 ]]
| + : goal2@reached_if
| + MYDIR=/some/path/tests
| + export MYDIR
| + cd /some/path/tests
| + A=aaa
| + export A
| + true
|   goal 'goal2' [already satisfied].
|   goal 'goal1' ...
| + : goal1
| + MYDIR=/some/path/tests
| + export MYDIR
| + cd /some/path/tests
| + A=aaa
| + export A
| + echo goal1
| goal1
|   goal 'default' ...
| + : default
| + MYDIR=/some/path/tests
| + export MYDIR
| + cd /some/path/tests
| + A=aaa
| + export A
| + echo A=aaa
| A=aaa

